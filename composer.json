{
    "name": "anon.to",
    "description": "anon.to - anonymous url redirector shortener",
    "keywords": [
        "anonymous",
        "redirector",
        "url shortener"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/lumen-framework": "5.0.*@dev",
        "vlucas/phpdotenv": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/"
        ],
        "files": [
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-update-cmd": [
            "composer dump-autoload -o"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
